{
  "name": "@jowomo/app",
  "version": "1.0.0",
  "description": "JOWOMO vernetzt Unternehmen für einen temporären Austausch von Personal. Flexibel. Innovativ. Krisengerecht.",
  "author": "",
  "private": true,
  "scripts": {
    "deploy": "serverless deploy && serverless client deploy --no-confirm --no-policy-change",
    "dev": "npx forever --minUptime 1000 --spinSleepTime 1000 node_modules/.bin/nuxt dev --spa",
    "dev-local": "USE_BETA_LOGO=true API_URL=http://localhost:5000/graphql npm run dev",
    "build": "nuxt build",
    "start": "nuxt start",
    "generate": "nuxt generate",
    "config:aws": "source ~/.bashrc && aws cloudformation describe-stacks --stack-name bee-to-bee-api-dev --query \"Stacks[0].Outputs\"> aws.json",
    "config:url": "source ~/.bashrc && aws ssm get-parameter --name /btb/dev/cloudfronturl --query Parameter.Value > aws_url.json",
    "generate-types": "graphql-codegen --config codegen.yml"
  },
  "dependencies": {
    "@aws-amplify/auth": "3.4.17",
    "@nuxtjs/apollo": "4.0.1-rc.5",
    "@nuxtjs/gtm": "2.4.0",
    "@nuxtjs/robots": "2.4.2",
    "@nuxtjs/sentry": "4.5.0",
    "@nuxtjs/sitemap": "2.4.0",
    "core-js": "3.8.2",
    "frontmatter-markdown-loader": "3.6.2",
    "graphql": "15.4.0",
    "graphql-tag": "2.11.0",
    "initials": "3.1.1",
    "lodash": "4.17.20",
    "markdown-it-anchor": "7.0.0",
    "markdown-it-external-links": "0.0.6",
    "nuxt": "2.14.12",
    "nuxt-class-component": "1.3.0",
    "nuxt-i18n": "6.17.0",
    "nuxt-property-decorator": "2.8.8",
    "nuxt-user-agent": "1.2.2",
    "sweetalert2": "10.13.0",
    "vue-apollo-decorator": "0.11.3",
    "vue-clamp": "0.3.1",
    "vue-infinite-loading": "2.4.5",
    "vue-mq": "1.0.1",
    "vue-property-decorator": "9.1.2",
    "vuelidate": "0.7.6",
    "vuelidate-property-decorators": "1.0.28"
  },
  "devDependencies": {
    "@graphql-codegen/cli": "1.20.0",
    "@graphql-codegen/typescript": "1.20.0",
    "@graphql-codegen/typescript-graphql-files-modules": "1.18.1",
    "@graphql-codegen/typescript-operations": "1.17.13",
    "@nuxt/typescript-build": "2.0.3",
    "@nuxt/types": "2.14.12",
    "@types/lodash": "4.14.167",
    "@types/vue-select": "2.5.1",
    "@types/vuelidate": "0.7.13",
    "forever": "3.0.4",
    "node-sass": "4.14.1",
    "sass-loader": "10.1.1",
    "serverless": "2.18.0",
    "serverless-cloudfront-distribution-certificate": "0.0.8",
    "serverless-finch": "2.6.0",
    "typescript": "4.1.3"
  }
}
